<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads the latest version of Microsoft's Skype for Business installer and imports it into your JSS.</string>
	<key>Identifier</key>
	<string>com.github.marcelpogorzelski.jss.SkypeForBusiness</string>
	<key>Input</key>
	<dict>
		<key>CATEGORY</key>
		<string>Microsoft</string>
    <key>PROD_NAME</key>
		<string>Skype for Business</string>
		<key>NAME</key>
		<string>SkypeForBusiness</string>
		<key>OS_REQUIREMENTS</key>
		<string>10.15.x,10.14.x,10.13.x,10.12.x</string>
		<key>POLICY_CATEGORY</key>
		<string>Microsoft</string>
		<key>CUSTOM_TRIGGER</key>
		<string>install-skypeforbusiness</string>
		<key>POLICY_TEMPLATE</key>
		<string>TriggerPolicyTemplate.xml</string>
		<key>SELF_SERVICE_DESCRIPTION</key>
		<string>Skype for Business is an instant-messaging client used with Skype for Business Server or with Skype for Business Online (available with Microsoft Office 365)</string>
		<key>SELF_SERVICE_ICON</key>
		<string>SkypeForBusiness.png</string>
	</dict>
	<key>MinimumVersion</key>
	<string>0.4.2</string>
	<key>ParentRecipe</key>
	<string>com.github.autopkg.download.SkypeForBusiness</string>
	<key>Process</key>
	<array>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>category</key>
				<string>%CATEGORY%</string>
				<key>groups</key>
				<array>
            <dict>
                <key>name</key>
                <string>UiT-Ansattemaskiner</string>
                <key>smart</key>
                <true/>
                <key>template_path</key>
                <string>UitGroupTemplate.xml</string>
            </dict>
            <dict>
                <key>name</key>
                <string>UiT-Datalabmaskiner</string>
                <key>smart</key>
                <true/>
                <key>template_path</key>
                <string>UitGroupTemplate.xml</string>
            </dict>
            <dict>
                <key>name</key>
                <string>UiT-Munkimaskiner</string>
                <key>smart</key>
                <true/>
                <key>template_path</key>
                <string>UitGroupTemplate.xml</string>
            </dict>
        </array>
				<key>os_requirements</key>
				<string>%OS_REQUIREMENTS%</string>
				<key>pkg_path</key>
				<string>%pathname%</string>
				<key>policy_category</key>
				<string>%POLICY_CATEGORY%</string>
				<key>policy_template</key>
				<string>%POLICY_TEMPLATE%</string>
				<key>prod_name</key>
				<string>%PROD_NAME%</string>
				<key>self_service_description</key>
				<string>%SELF_SERVICE_DESCRIPTION%</string>
				<key>self_service_icon</key>
				<string>%SELF_SERVICE_ICON%</string>
			</dict>
			<key>Processor</key>
			<string>JSSImporter</string>
		</dict>
	</array>
</dict>
</plist>
